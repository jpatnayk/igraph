samp <- read.csv(file=file.choose())
print(samp)
lapply(samp, length)
sapply(samp, length)
lapply(samp, mean)
lapply(samp, range)
sapply(samp, mean)
subset(samp, select=Ozone)
subset(samp, subset=(Temp > 35))
temp <- edit(samp)
samp <- temp
clean <- na.omit(samp)
samp
sum(na.omit(samp))
cumsum(na.omit(samp))
fix(samp)
samp <- na.omit(samp)
